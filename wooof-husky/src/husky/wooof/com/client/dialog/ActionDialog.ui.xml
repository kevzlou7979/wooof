<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:h="urn:import:husky.wooof.com.client.ui">
	<ui:with field='res' type='husky.wooof.com.client.resources.HuskyResources' />
	<ui:style>
		.btnAction{
			display: inline-block;
			vertical-align: middle;
			width: 100px;
		}
		.lblTitle{
		
		}
	</ui:style>
	<g:HTMLPanel>
	    <g:Image resource="{res.ic_action_warning}"/>
	    <g:Label ui:field="lblTitle" text="Title"/>
	    <g:Label ui:field="lblDesc" text="Description"/>
	    <h:HuskyButton ui:field="btnOk" text="Okay" addStyleNames="{style.btnAction}"/>
		    <h:HuskyButton ui:field="btnCancel" text="Cancel" addStyleNames="{res.huskycss.huskyButtonWhite} {style.btnAction}"/>
	</g:HTMLPanel>
</ui:UiBinder> 