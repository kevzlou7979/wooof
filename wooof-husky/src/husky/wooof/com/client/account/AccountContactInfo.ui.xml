<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:h="urn:import:husky.wooof.com.client.ui">
	<ui:with field='res' type='husky.wooof.com.client.resources.HuskyResources' />
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel addStyleNames="{res.huskycss.accountPanel}">
		<h:HuskyTitle title="Contact Information"  resource="{res.ic_account_contact_info}" color="4086fa" border="false" imageSize="50px" fontSize="1.5"/>
	
		<g:Label text="Home" addStyleNames="{res.huskycss.huskyBasicTitle}"/>
		<g:Label text="How can people reach you by home" addStyleNames="{res.huskycss.huskyBasicDescription}"/>
		
		<g:Label text="Work" addStyleNames="{res.huskycss.huskyBasicTitle}"/>
		<g:Label text="How can pople reach you by work" addStyleNames="{res.huskycss.huskyBasicDescription}"/>
		
	</g:HTMLPanel>
</ui:UiBinder> 