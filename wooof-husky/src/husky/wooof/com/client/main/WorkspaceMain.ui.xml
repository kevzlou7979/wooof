<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:h="urn:import:husky.wooof.com.client.ui">
	<ui:with field='res'
		type='husky.wooof.com.client.resources.HuskyResources' />

	<ui:style>
	.activeUsersPanel {
		font-size: 0.3em;
		border-bottom: 1px solid #e9e9e9;
		position: relative;
		padding-top: 20px;
	}
	
	.ic_avatar {
		position: absolute;
		top: -10px;
	}
	</ui:style>
	<g:HTMLPanel ui:field="panel">
		<g:HTMLPanel ui:field="activeUsersPanel" addStyleNames="{style.activeUsersPanel}" />

		<g:HTMLPanel ui:field="mainPanel">

		</g:HTMLPanel>

		<g:HTMLPanel ui:field="lessonPanel">

		</g:HTMLPanel>

	</g:HTMLPanel>
</ui:UiBinder> 