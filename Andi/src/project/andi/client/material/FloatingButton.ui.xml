<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:project.andi.client.material"
	xmlns:d="urn:import:project.andi.client.modal">
	<ui:style>
		.floatingButton{
			position: fixed;
			bottom: 10px;
			right: 10px;
			background: #a72773;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:FocusPanel ui:field="btnFloating">
			<g:HTMLPanel>
				<a class="{style.floatingButton} btn-floating btn-large waves-effect waves-light pink darken-4 modal-trigger" ><i class="mdi-content-add"></i></a>
			</g:HTMLPanel>
		</g:FocusPanel>
	</g:HTMLPanel>
</ui:UiBinder> 