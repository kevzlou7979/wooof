<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:project.andi.client.material">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel ui:field="panel">
		<h4>Story</h4>
		<p>Fill up all the required fields</p>
		
		<m:MaterialTextBox ui:field="txtTitle" placeholder="Story Title"/>
		<m:MaterialTextBox ui:field="txtDescription" placeholder="Story Description"/>
		<m:MaterialTextBox ui:field="txtCode" placeholder="Story Code"/>
		<m:MaterialTextBox ui:field="txtPassword" type="password" placeholder="Story Password"/>
		
		<g:Label ui:field="lblCreate" text="Create" addStyleNames="waves-effect btn-flat"/>
		
	</g:HTMLPanel>
</ui:UiBinder> 