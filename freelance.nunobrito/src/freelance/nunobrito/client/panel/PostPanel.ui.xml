	<ui:with field='res' type='freelance.nunobrito.client.resouces.DotClickResources' />
	<ui:style>
	.panel{
		width: 80%;
		margin-left: 300px;
	}
	.postContentPanel{
		width: 100%;
	}
	</ui:style>
	<g:HTMLPanel addStyleNames="{res.dotclickcss.inline} {style.panel}">
